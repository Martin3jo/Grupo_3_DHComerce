<% for (let i=0; i < categorias.length; i++) { %>
    <div>
        <strong>
            <%= categorias[i].nombre %>
        </strong>

    </div>
    <section class="productos-main-caja">
        <% for(let j=0; j < categorias[i].Producto.length; j++) { %>
            <article class="productos-caja">
                <a href="/productos/detalle/<%= categorias[i].Producto[j].idproducto %>">
                    <div class="productos-caja-1">
                        <img class="avatar-imagen" src="/img/avatars/<%= categorias[i].Producto[j].avatar %>"
                            alt="avatar">
                    </div>
                    <div class="productos-caja-2">
                        <%= categorias[i].Producto[j].marca %><br>
                            $<%= categorias[i].Producto[j].precio %>
                    </div>
                </a>
            </article>
            <% } %>

    </section>
    <% } %>